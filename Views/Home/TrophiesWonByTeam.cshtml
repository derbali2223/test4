@model IEnumerable<Exercices07.Models.Entities.Trophy>

@{
    ViewData["Title"] = "TrophiesWonByTeam";
}

<h1>Trophées gagnés par l'équie @ViewBag.team.Name</h1>

<p>
    <a asp-action="CreateTrophy" asp-route-id="@ViewBag.team.Id">Ajouter un trophée</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Year)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Year)
            </td>
        </tr>
}
    </tbody>
</table>
